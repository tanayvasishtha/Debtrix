{
  "name": "debtrix",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests yet\" && exit 0",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production next build",
    "start:production": "NODE_ENV=production next start",
    "restart": "rm -rf .next && npm run dev",
    "restart:win": "rmdir /s .next && npm run dev",
    "export": "next export",
    "clean": "rm -rf .next out",
    "postbuild": "echo 'âœ… Build completed successfully'",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=.next",
    "deploy:railway": "railway deploy",
    "health": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
